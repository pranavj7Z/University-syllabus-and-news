<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="false"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="8dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".MainActivity"
        android:background="@drawable/ktu77777">


    <LinearLayout
        android:id="@+id/firstLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp"
        android:paddingEnd="5dp"
        android:paddingStart="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.ktuhelper.s1s2"
            android:paddingEnd="10dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="S3"
                android:id="@+id/button5"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="lac"
                android:layout_marginBottom="0dp"
                android:text="Linear Algebra &amp; Complex Analysis "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="net"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Network Theory "
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="sd"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Solid State Devices " />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="ecirc"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Electronic Circuits " />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="business"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Business Economics/Life Skills"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="lcd"
                android:text="Logic Circuit Design"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:id="@+id/button9"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="ecedcl"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Electronic Devices &amp; Circuits Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="edal"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Electronic Design Automation Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/secondView"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/thirdLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.ktuhelper.s1s2"
            android:paddingEnd="10dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="s4"
                android:id="@+id/button3"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:onClick="SAS"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:text="Signals &amp; Systems "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="AIC"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Analog Integrated Circuits "
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:onClick="CO"
                android:textColor="#ffffff"
                android:text="Computer Organization" />
            <Button
                android:background="#50000000"
                android:onClick="ACE"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Analog Communication Engineering " />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center"
                android:onClick="PRN"
                android:text="Probability, Random Processes and Numerical Methods "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="LS"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Life Skills/Business Economics "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:onClick="AICL"
                android:text="Analog Integrated Circuits Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="LCDL"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Logic Circuit Design Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Electronics &amp; communication"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#e83333"
        android:textColor="#ffffff" />
</RelativeLayout>
</android.support.v4.widget.NestedScrollView>
    </android.support.design.widget.CoordinatorLayout>