<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="false"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_marginTop="?attr/actionBarSize"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".MainActivity"
        android:background="@drawable/ktu77777">
    <LinearLayout
        android:id="@+id/firstLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.myktu.s1s2"
            >

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="S3"
                android:id="@+id/button5"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="lac"
                android:layout_marginBottom="0dp"
                android:text="Linear Algebra &amp; Complex Analysis "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="can"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Circuits and Networks "
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="aec"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Analog Electronic Circuits" />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="dcm"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="DC Machines and Transformers" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="cpm"
                android:layout_gravity="center"
                android:text="Computer Programming"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="business"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Business Economics"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:id="@+id/button9"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="eeecl"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Electronic Circuits Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="cpl"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Programming Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/secondView"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/thirdLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.myktu.s1s2"
          >

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="s4"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:onClick="SAIM"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:text="Synchronous and Induction Machines"
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:onClick="DEALD"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Digital Electronics and Logic Design"
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:onClick="MS"
                android:textColor="#ffffff"
                android:text="Material Science" />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="MAI"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Measurements and Instrumentation " />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="PTN"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Probability Distributions, Transforms and Numerical Methods" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="LS"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Life Skills"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:id="@+id/button10"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:onClick="EML1"
                android:text="Electrical Machines Lab I "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="CAML"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Circuits and Measurements Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Electrical and electronics"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#e83333"
        android:textColor="#ffffff" />
</RelativeLayout>
</android.support.v4.widget.NestedScrollView>
    </android.support.design.widget.CoordinatorLayout>