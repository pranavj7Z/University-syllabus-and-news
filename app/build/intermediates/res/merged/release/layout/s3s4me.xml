<android.support.design.widget.CoordinatorLayout
android:layout_width="match_parent"
android:layout_height="match_parent"
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:fillViewport="false"
xmlns:app="http://schemas.android.com/apk/res-auto">

<android.support.design.widget.AppBarLayout
    android:id="@+id/appbar"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="8dp">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </android.support.v7.widget.Toolbar>

</android.support.design.widget.AppBarLayout>
<android.support.v4.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".MainActivity"
        android:background="@drawable/ktu77777">


    <LinearLayout
        android:id="@+id/firstLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="5dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.myktu.s1s2"
            android:paddingEnd="10dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="S3"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="lac"
                android:layout_marginBottom="0dp"
                android:text="Linear Algebra &amp; Complex Analysis "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="mes"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Mechanics of Solids"
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="mef"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Mechanics of Fluids" />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="thermo"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Thermodynamics" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="metal"
                android:layout_gravity="center"
                android:text="Metallurgy and Materials Engineering "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="business"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Business Economics"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="cad"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Computer Aided Machine Drawing Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="Material"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Material Testing Lab "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/secondView"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/thirdLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.myktu.s1s2"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="20dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="s4"
                android:id="@+id/button5"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:onClick="AMS"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:text="Advanced Mechanics of Solids "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:onClick="TE"
                android:layout_marginTop="5dp"
                android:text="Thermal Engineering "
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:onClick="FMACHINE"
                android:textColor="#ffffff"
                android:text="Fluid Machinery" />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:onClick="MT"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Manufacturing Technology" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center"
                android:onClick="PRN"
                android:text="Probability, Random Processes and Numerical Methods "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="LS"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Life Skills"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:id="@+id/button9"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:onClick="TEL"
                android:layout_gravity="center"
                android:text="Thermal Engineering Lab "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginRight="0dp"
                android:onClick="FMML"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Fluid Mechanics &amp; Machines Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Mechanical"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#e83333"
        android:textColor="#ffffff" />
</RelativeLayout>
</android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>