<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pranavj7.ktuhelper">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <activity
            android:name="com.example.pranavj7.ktuhelper.Main">
            <intent-filter >
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".s1s2"
            android:parentActivityName="com.example.pranavj7.ktuhelper.Main"
            android:label="Subjects">

            <intent-filter>
                <action android:name="com.example.package_path.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".s3s4"
            android:parentActivityName="com.example.pranavj7.ktuhelper.Main"
        android:label="Branches">

            <intent-filter>
                <action android:name="com.example.package_path.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Network"
            android:parentActivityName="com.example.pranavj7.ktuhelper.s3s4"
            android:label="Subjects">

            <intent-filter>
                <action android:name="com.example.package_path.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pranavj7.ktuhelper.DetailActivity"
            android:parentActivityName="com.example.pranavj7.ktuhelper.Network"
            android:label="Syllabus">
            <intent-filter >
                <action android:name="com.example.package_path.SecondActivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.pranavj7.ktuhelper.DetailActivitys1s2"
            android:parentActivityName="com.example.pranavj7.ktuhelper.s1s2"
            android:label="Syllabus">
            <intent-filter >
                <action android:name="com.example.package_path.SecondActivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pranavj7.ktuhelper.DetailQactivity"
            android:label="Questions by year">
            <intent-filter >
                <action android:name="com.example.package_path.SecondActivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".qst_activity"
            android:label="Question papers">
            <intent-filter >
                <action android:name="com.example.package_path.qst_Activity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
