<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="false"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="8dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>
<android.support.v4.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".MainActivity"
    android:background="@drawable/ktu77777">

    <LinearLayout
        android:id="@+id/firstLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp"
        android:paddingEnd="5dp"
        android:paddingStart="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.ktuhelper.s1s2"
            android:paddingEnd="10dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="S3"
                android:id="@+id/button5"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="lac"
                android:layout_marginBottom="0dp"
                android:text="Linear Algebra &amp; Complex Analysis "
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="solids"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Mechanics of Solids"
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"

                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="mech1"
                android:layout_marginBottom="0dp"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Fluid Mechanics I " />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="enggeo"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text="Engineering Geology" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="business"
                android:layout_gravity="center"
                android:text="Business Economics"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="survey"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Surveying "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:id="@+id/button9"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="draft"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Civil Engineering Drafting Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="surveylab"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Surveying Lab"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/secondView"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/thirdLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/secondView"
        android:orientation="vertical"
        android:layout_marginTop="70dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content"
            tools:context="com.example.pranavj7.ktuhelper.s1s2"
            android:paddingEnd="10dp">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="s4"
                android:id="@+id/button6"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:onClick="SA1"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:text=" Structural Analysis I"
                android:layout_marginTop="5dp"
                android:layout_gravity="center" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="CT"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="5dp"
                android:text="Construction Technology"
                android:textColor="#ffffff" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginBottom="0dp"
                android:onClick="PTN"
                android:layout_gravity="top|center"
                android:layout_marginTop="5dp"
                android:textColor="#ffffff"
                android:text="Probability Distributions, Transforms and Numerical Methods" />
            <Button
                android:background="#50000000"
                android:textColor="#ffffff"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="FM2"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="0dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="0dp"
                android:text=" Fluid Mechanics II " />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center"
                android:onClick="LS"
                android:text="Life Skills"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="GE1"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Geotechnical Engineering I"
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />


            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="lab"
                android:layout_gravity="center_horizontal" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:onClick="MTL1"
                android:text="Materials Testing Lab I "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

            <Button
                android:background="#50000000"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:onClick="FML"
                android:layout_marginRight="0dp"
                android:layout_marginLeft="0dp"
                android:layout_gravity="center"
                android:text="Fluid Mechanics Lab "
                android:textColor="#ffffff"
                android:layout_marginTop="5dp" />

        </LinearLayout>

    </LinearLayout>

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:text="Civil"
        android:id="@+id/button4"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="#e83333"
        android:textColor="#ffffff" />
</RelativeLayout>
</android.support.v4.widget.NestedScrollView>
    </android.support.design.widget.CoordinatorLayout>