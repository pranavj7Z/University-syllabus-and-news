<android.support.design.widget.CoordinatorLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="false"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:elevation="8dp">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

<RelativeLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         tools:context=".MainActivity"
         android:background="@drawable/ktu77777">

         <LinearLayout
             android:id="@+id/firstLayout"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_toLeftOf="@+id/secondView"
             android:orientation="vertical"
             android:layout_marginTop="70dp"
             android:paddingEnd="5dp"
             android:paddingStart="10dp">

             <LinearLayout
                 android:layout_width="match_parent"
                 android:orientation="vertical"
                 android:layout_height="wrap_content"
                 tools:context="com.example.pranavj7.ktuhelper.s1s2"
                 android:paddingEnd="10dp">

                 <Button
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="S3"
                     android:id="@+id/button5"
                     android:layout_gravity="center_horizontal" />

                 <Button
                     android:background="#50000000"
                     android:textColor="#ffffff"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="stld"
                     android:layout_marginBottom="0dp"
                     android:text="Switching theory and logic design"
                     android:layout_marginTop="5dp"
                     android:layout_gravity="center" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="ds"
                     android:layout_gravity="center_horizontal"
                     android:layout_marginLeft="0dp"
                     android:layout_marginTop="5dp"
                     android:text="Data Structures"
                     android:textColor="#ffffff" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="lac"
                     android:layout_marginBottom="0dp"
                     android:layout_gravity="top|center"
                     android:layout_marginTop="5dp"
                     android:textColor="#ffffff"
                     android:text="Linear algebra and complex numbers" />
                 <Button
                     android:background="#50000000"
                     android:textColor="#ffffff"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="business"
                     android:layout_gravity="center_horizontal"
                     android:layout_marginRight="0dp"
                     android:layout_marginTop="5dp"
                     android:layout_marginLeft="0dp"
                     android:text="Business Economics" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="dcs"
                     android:layout_gravity="center"
                     android:text="Discrete computational structures"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="edc"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:text="Electronic devices and circuits"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />


                 <Button
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="lab"
                     android:id="@+id/button9"
                     android:layout_gravity="center_horizontal" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="edclab"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:text="Electronics lab"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="dslab"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:text="Data structure lab"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

             </LinearLayout>

         </LinearLayout>

         <View
             android:id="@+id/secondView"
             android:layout_width="0dp"
             android:layout_height="match_parent"
             android:layout_centerHorizontal="true" />

         <LinearLayout
             android:id="@+id/thirdLayout"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:layout_toRightOf="@+id/secondView"
             android:orientation="vertical"
             android:layout_marginTop="70dp">

             <LinearLayout
                 android:layout_width="match_parent"
                 android:orientation="vertical"
                 android:layout_height="wrap_content"
                 tools:context="com.example.pranavj7.ktuhelper.s1s2"
                 android:paddingEnd="10dp">

                 <Button
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="s4"
                     android:id="@+id/button6"
                     android:layout_gravity="center_horizontal" />

                 <Button
                     android:background="#50000000"
                     android:textColor="#ffffff"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="OS"
                     android:layout_marginBottom="0dp"
                     android:text="Operating Systems"
                     android:layout_marginTop="5dp"
                     android:layout_gravity="center" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="COA"
                     android:layout_gravity="center_horizontal"
                     android:layout_marginLeft="0dp"
                     android:layout_marginTop="5dp"
                     android:text="Computer Organization and Architecture"
                     android:textColor="#ffffff" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="PTN"
                     android:layout_marginBottom="0dp"
                     android:layout_gravity="top|center"
                     android:layout_marginTop="5dp"
                     android:textColor="#ffffff"
                     android:text="Probability Distributions, Transforms and Numerical Methods" />
                 <Button
                     android:background="#50000000"
                     android:textColor="#ffffff"
                     android:onClick="OOD"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:layout_gravity="center_horizontal"
                     android:layout_marginRight="0dp"
                     android:layout_marginTop="5dp"
                     android:layout_marginLeft="0dp"
                     android:text="Object Oriented Design and Programming " />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="PDD"
                     android:layout_gravity="center"
                     android:text="Principles of Database Design"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="LS"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:text="Life Skills "
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />


                 <Button
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="lab"
                     android:id="@+id/button8"
                     android:layout_gravity="center_horizontal" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:onClick="FOSS"
                     android:text="Free and Open Source Software Lab"
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

                 <Button
                     android:background="#50000000"
                     android:layout_width="match_parent"
                     android:layout_height="90dp"
                     android:onClick="DSL"
                     android:layout_marginRight="0dp"
                     android:layout_marginLeft="0dp"
                     android:layout_gravity="center"
                     android:text="Digital Systems Lab "
                     android:textColor="#ffffff"
                     android:layout_marginTop="5dp" />

             </LinearLayout>

         </LinearLayout>

         <Button
             android:layout_width="200dp"
             android:layout_height="50dp"
             android:text="computer Science"
             android:id="@+id/button4"
             android:layout_alignParentTop="true"
             android:layout_centerHorizontal="true"
             android:background="#e83333"
             android:textColor="#ffffff" />
     </RelativeLayout>
        </android.support.v4.widget.NestedScrollView>
</android.support.design.widget.CoordinatorLayout>